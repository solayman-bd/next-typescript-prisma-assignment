  ceo                                        String
  title                                      String
  date                                       String
  company_rel                                Int
  insider_rel                                Int
  outsider_rel                               Int
  ceo_rel                                    Int
  company_sentiment                          Float
  insider_sentiment                          Float?
  outsider_sentiment                         Float?
  ceo_sentiment                              Float?
  GHG_emissions_similarity                   Float
  GHG_emissions_sentiment                    Float?
